<project>
    <groupId>edu.jhu.aa</groupId>
    <artifactId>Modulo7</artifactId>
    <modelVersion>4.0.0</modelVersion>
    <name>Modulo7</name>
    <packaging>jar</packaging>
    <version>1.0</version>
	<dependencies>
		<dependency>
			<groupId>org.jsoup</groupId>
			<artifactId>jsoup</artifactId>
			<version>1.7.2</version>
		</dependency>
	</dependencies>
    <build>
        <plugins>
            <plugin>
                <artifactId>maven-compiler-plugin</artifactId>
                <configuration>
                    <source>1.7</source>
                    <target>1.7</target>
                </configuration>
            </plugin>
        </plugins>
    </build>
</project>
